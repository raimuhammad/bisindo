{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client", "node"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@colors": ["ui/assets/theme/base/colors.ts"],
      "@globals": ["ui/assets/theme/base/globals.ts"],
      "@typography": ["ui/assets/theme/base/typography.ts"],
      "@box-shadow": ["ui/assets/theme/base/boxShadows.ts"],
      "@borders": ["ui/assets/theme/base/borders.ts"],
      "@breakpoints": ["ui/assets/theme/base/breakpoints.ts"],
      "@theme-functions/*": ["ui/assets/theme/functions/*"],
      "@admin-routes": ["ui/routes/admin.ts"],
      "@student-routes": ["ui/routes/user.ts"],
      "@admin-pages/*": ["ui/pages/admin/*"],
      "@student-pages/*": ["ui/pages/student/*"],
      "@student-page": ["ui/pages/student"],
      "@root/*": ["ui/*"],
      "@components/*": ["ui/components/*"],
      "@guest-page": ["ui/pages/guest/index.ts"],
      "@hooks/*": ["ui/hooks/*"],
      "@layout/*": ["ui/layout/*"],
      "@models/*": ["ui/models/*"],
      "@model": ["ui/models"],
      "@providers/*": ["ui/providers/*"],
      "@service-provider/*": ["ui/providers/service-provider/*"],
      "@shared/*": ["ui/shared/*"],
      "@pages/*": ["ui/pages/*"],
      "@utils/*": ["ui/utils/*"],
      "@services/*": ["services/*"],
      "@root-model": ["ui/models/RootStore.base.ts"]
    }
  },
  "include": ["ui", "vite.config.ts", "cypress"],
  "exclude": ["node_modules", "ui/src.old"]
}
